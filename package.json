{
  "name": "barcode-reader-wasm",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "build:start": "rollup -c --environment RUN_SERVER:true",
    "build:wasm:prod": "yarn build:wasm -O3",
    "build:wasm": "emcc -s WASM=1 -s \"EXPORTED_FUNCTIONS=['_decode_qr_code']\" -I ./src/wasm/quirc ./src/wasm/barcode.c ./src/wasm/quirc/*.c -o ./src/wasm/barcode.html --no-entry -s 'EXTRA_EXPORTED_RUNTIME_METHODS=[\"cwrap\"]'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-wasm": "^5.1.2",
    "@surma/rollup-plugin-off-main-thread": "^2.0.0",
    "@web/rollup-plugin-html": "^1.4.0",
    "rollup": "^2.39.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.32.3"
  },
  "browserslist": [
    "last 2 years",
    "> 0.2%",
    "not dead"
  ],
  "dependencies": {
    "comlink": "^4.3.0",
    "jsqr": "^1.3.1"
  }
}
